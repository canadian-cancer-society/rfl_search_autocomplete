          jQuery(document).ready(function () {
          jQuery.noConflict();
    
              //---------------------------------------------------------
              // Override default ccsLocator options for testing.
              //---------------------------------------------------------
    
             var query = jQuery.parseQuery();
             var language = "fr_CA";
                 if (language === "") {
                      language = "en_CA";
                     }
                 //Set the participation type variable
                     
                           var pType="";
                     
                console.log('ptype 2' + pType);
              jQuerylocator = jQuery("#ccs-locator-container").ccsLocator({
                  searchText: query.address,      // The initial search text used to retrieve locations before rendering.
                  //language: "fr_CA",           // Renders French if language starts with "fr". Otherwise, renders English.
                  language: "",           // Renders French if language starts with "fr". Otherwise, renders English.
                  //language: query.lang,           // Renders French if language starts with "fr". Otherwise, renders English.
                  //year: "2013",
                  className: "ccs-locator",       // The class name assigned to the plugin outermost div.
                  radiusOptions: [10, 25, 50, 100],       // Limits the radius options to 50 and 100km.
    //                    service: "relay locations",     // Sitecore service page (required).
                  service: "relay-locations",     // Sitecore service page (required).
                  markerIconUrl: "//$host/resources/services/images/rfl-marker.png",   // This is the url of the relay for life marker icon.
                  numbered: true,               // If true, create a numbered list, otherwise create an unordered list.
    
                  secure: true,                  // Set to true to cause plugin to submit https requests.
    
                  onRenderItem: function (args) {
                      // This callback is fired to render a location in the plugin.  Add code to render:
                      //   1) the sidebar list item for the location, and
                      //   2) html for the popup info window associated with the map marker.
                      //
                      // The args parameter has the following properties:
                      //   args.source:    The locator object.
                      //   args.item:      The current location data to be rendered.
                      //   args.index:     The index of the current location in the array of retrieved locations.
                      //   args.li:        Set to the list item to be rendered into the sidebar in this property.
                      //   args.popupHtml: Set to the html for the marker popup window in this property.
                      var item = args.item,
    
                          address = "",
                          addressBreak = "",
                          city = "",
                          cityBreak = "",
                          comment = "",
                          commentBreak = "",
                          date = "",
                          dateBreak = "",
                          link = "";
                      console.log('args.index 2' + args.index)
                      if (args.index <= 4) {
                          if (item.Address.length > 0) {
                              address = jQuery("<span class='location-address'/>").text(item.Address);
                              addressBreak = jQuery("<br>");
                          }
                          if (item.City.length > 0) {
                              city = jQuery("<span class='location-city'/>").text(item.City);
                              cityBreak = ", ";
                          }
                          if (item.EventDate.length > 0) {
                              date = jQuery("<span class='location-date'/>").html(item.EventDate);
                              dateBreak = jQuery("<br>");
                          }
    //HANDLE COMMENTS
                          if (item.Comment.length > 0) {
    //                        console.log('ptype ' + pType);
    //                        console.log('event comment ' + item.Comment);
    
                              var regClosedText = '';
                              var lumClosedText = '';
                              var volClosedText = '';
    
                              if (language === 'fr_CA'){
                                  regClosedText = 'L&#39;inscription est  termin&#233;e.';
                                  lumClosedText = 'La vente de luminaires est termin&#233;e.';
                                  volClosedText = 'L&#39;inscription des bénévoles est termin&#233;e.';
                              }
                              else {
                                  regClosedText = 'Registration is closed.';
                                  lumClosedText = 'Luminary purchasing is closed.';
                                  volClosedText = 'Volunteer registration is closed.';
                              }
    
                              // If all three closed messages are present
                              if (item.Comment.indexOf('reg closed, vol closed, lum closed') != -1){
                                   // If there is a pType and all three messages
                                   if (pType.indexOf('reg') != -1){
                                      comment = jQuery("<span class='location-comment'/>").html(regClosedText);
                                      commentBreak = jQuery("<br>");
                                   }
                                   else if (pType === 'pledgeEvent') {
                                       comment = '';
                                       commentBreak = '';
                                   }
                                   else if (pType.indexOf('luminary') != -1){
                                      comment = jQuery("<span class='location-comment'/>").html(lumClosedText);
                                      commentBreak = jQuery("<br>");
                                   }
                                   else if (pType.indexOf('volunteer') != -1){
                                      comment = jQuery("<span class='location-comment'/>").html(volClosedText);
                                      commentBreak = jQuery("<br>");
                                   }
                                   else {
                                      comment = jQuery("<span class='location-comment'/>").html(regClosedText + '<br />' + lumClosedText + '<br />' + volClosedText);
                                      commentBreak = jQuery("<br>");
                                   }
                                }
                              else {
                                 // If pType is blank 
                                 if (pType === ''){
                                   // If all three messages are present 
                                   if (item.Comment.indexOf('reg closed, lum closed, vol closed') != -1){
                                       comment = jQuery("<span class='location-comment'/>").html(regClosedText + '<br />' + lumClosedText + '<br />' + volClosedText);
                                       commentBreak = jQuery("<br>");
                                   }
                                   else if (item.Comment.indexOf('reg closed') != -1){
                                       comment = '';
                                       commentBreak = '';
                                   }
                                   else if (item.Comment.indexOf('lum closed') != -1){
                                       comment = '';
                                       commentBreak = '';
                                   }
                                   else if (item.Comment.indexOf('vol closed') != -1){
                                       comment = '';
                                       commentBreak = '';
                                   }
                                   else { 
                                       comment = jQuery("<span class='location-comment'/>").html(item.Comment);
                                       commentBreak = jQuery("<br>");
                                   }
                                 }
                                // If pType starts with reg
                                if (pType.indexOf('reg') != -1){
                                      if (item.Comment.indexOf('reg closed') != -1){
                                         comment = jQuery("<span class='location-comment'/>").html(regClosedText);
                                         commentBreak = jQuery("<br>");
                                      }
                                      else if (item.Comment.indexOf('lum closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('lum closed, vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else {
                                         comment = jQuery("<span class='location-comment'/>").html(item.Comment);
                                         commentBreak = jQuery("<br>");
                                      }
                                   }
    
                                // If pType is pledgeEvent
                                if (pType.indexOf('pledgeEvent') != -1){
                                      if (item.Comment.indexOf('reg closed') != -1){
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('lum closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('lum closed, vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('reg closed, lum closed, vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else {
                                         comment = jQuery("<span class='location-comment'/>").html(item.Comment);
                                         commentBreak = jQuery("<br>");
                                      }
                                   }
    
                                // If pType is luminary
                                 if (pType.indexOf('luminary') != -1){
                                      if (item.Comment.indexOf('lum closed') != -1){
                                         comment = jQuery("<span class='location-comment'/>").html(lumClosedText);
                                         commentBreak = jQuery("<br>");
                                      }
                                      else if (item.Comment.indexOf('reg closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('reg closed, vol closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else {
                                         comment = jQuery("<span class='location-comment'/>").html(item.Comment);
                                         commentBreak = jQuery("<br>");
                                      }
                                   }
    
                                // If pType is volunteer 
                                 if (pType.indexOf('volunteer') != -1){
                                      if (item.Comment.indexOf('vol closed') != -1){
                                         comment = jQuery("<span class='location-comment'/>").html(volClosedText);
                                         commentBreak = jQuery("<br>");
                                      }
                                      else if (item.Comment.indexOf('reg closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('lum closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else if (item.Comment.indexOf('reg closed, lum closed') != -1) { 
                                         comment = '';
                                         commentBreak = '';
                                      }
                                      else {
                                         comment = jQuery("<span class='location-comment'/>").html(item.Comment);
                                         commentBreak = jQuery("<br>");
                                      }
                                   }
                               }
    
    //console.log(item.Comment.indexOf('Registration is closed'));
    //                        comment = jQuery("<span class='location-comment'/>").html(item.Comment);
    //                        commentBreak = jQuery("<br>");
                      }
    //HANDLE LINKS
                          var linkText = '';
                          // Set the link text per language
                          if (language === 'fr_CA'){
                             linkText = 'Sélectionnez cet événement.';
                          }
                          else {
                             linkText = 'Select this event.';
                          }
        
