    <script type="text/javascript">
    jQuery(document).ready(function() {
        
    //	jQuery('div.responsive a.next-step').wrap('<div class="corners"><div class="don-gradient"></div></div>');
    //	jQuery('div.responsive a.secondary-step').wrap('<div class="corners"><div class="reg-gradient"></div></div>');
    
        
            //hide the thermometer if percent achieved is less than 50
            var percentAchieved = '40';
//            console.log('percentAchieved ' +percentAchieved)
            
				//Hide the Community thermometer until 50% of goal is achieved
	            if (percentAchieved < 50) {
	                jQuery('.therm').hide();
	            }
			
        
    
        // update the percent of goal achieved label
        
            jQuery('.amount-percentage-label').html('de l\'objectif élevé');
        
		
		//open the sponsor links in a new tab
		if (jQuery('.tr_sponsorship_logo').length > 0) {
			jQuery('.tr_sponsorship_logo').each(function(){
				if (jQuery(this).children().first().hasClass('tr_sponsorship_logo_url') === true) {
					jQuery(this).children().first().attr('target','blank');
				}
			});
		}

        // pull the event news content from the custom page
        jQuery.ajax({
            
                
					
	                    url:'TR?pg=informational&fr_id=20321&type=fr_informational&sid=7980&s_locale=fr_CA&pgwrap=n',
					
                
            
            type:'post',
            dataType:'text',
            cache: 'false',
            success:function(data) {
                var storyList = [];
                var news = data;
				var newsSplit = news.split('<div class="local-news">')[1];
				if (newsSplit){
					var localNews = newsSplit.split('<div class="natl-news">')[0];
//					console.log('localNews ' + localNews);

					if (localNews){
						var stories = localNews.split('<div class="story">');
//						console.log('stories ' + stories);
						var storyList = [];
//						console.log('stories.length ' + stories.length);
						jQuery('.details-right').append('<div class="local-news"></div>');
						for (i = 1; i < stories.length; i++) {
							storyList.push(stories[i]);
						}

						jQuery(storyList).each(function(){
							var headlineCheck = this.split('<div class="headline type-box" style="outline: 1px solid red; margin: 2px; padding: 10px;">')[1];
							if (headlineCheck){
								var checkSplit = headlineCheck.split('</div>')[0];

								if (checkSplit.length > 0){
									var headline = this.split('<div class="headline type-box" style="outline: 1px solid red; margin: 2px; padding: 10px;">')[1];
									var headlineSplit = headline.split('</div>')[0];
	//								console.log('headlinesplit ' + headlineSplit.length);
									if (headlineSplit.length > 0) {
										var bodyContent = this.split('<div class="body type-box" style="outline: 1px solid red; margin: 2px; padding: 10px;">');
										var bodySplit = bodyContent[1].split('</div>')[0];
										var bodyClean = jQuery(bodySplit).text();
//										jQuery('.local-news').append('<div class="story"><div class="headline">'+headlineSplit+'</div><div class="body">'+bodySplit+'</div></div>');
										jQuery('.local-news').append('<div class="story"><div class="headline">'+headlineSplit+'</div><div class="body">'+bodyClean+'</div></div>');
									}
								}
							}
						});
						
							jQuery('.local-news').append('<div class="news-footer">'
							+ ''
							+ '<p><a href="https://twitter.com/search?q=%23Acceptezlebâton" target="blank">#Acceptezlebâton</a><br />'
							+ '<a href="https://twitter.com/search?q=%23VivreleRelais" target="blank">#VivreleRelais </a><br />'
							+ '<a href="https://twitter.com/search?q=%23relaispourlavie" target="blank">#Relaispourlavie</a></p></div>');
						
						
						if (jQuery('.local-news .story').length > 0) {
							var truncateNews = function() {
								var numStories = jQuery('.local-news .story .headline').length;
	//							console.log('numStories ' + numStories);
								var truncateLength = 140;
								if (numStories > 1) {
									truncateLength = 22;
								}
								jQuery('.local-news .story .headline').each(function(){
									var headline = jQuery(this).html();
									headline = headline.replace(/<img[^>]*>/g,"");
									if (headline.length >= 27) {
										var trimmedHeadline = headline.substring(0, 27);
										jQuery(this).html(trimmedHeadline + '...');
									}
								});
								jQuery('.local-news .story .body').each(function(){
									var content = jQuery(this).html();
									content = content.replace(/<img[^>]*>/g,"");
									var trimmedNews = content;
									if (content.length >= truncateLength) {
										trimmedNews = content.substring(0, truncateLength);
									}
									
										
											
												jQuery(this).html(trimmedNews + '... <a href="TR?pg=informational&fr_id=20321&type=fr_informational&sid=7980">Lire la suite</a>');
											
										
									
								});
							}

							truncateNews();
						}
						else {
							jQuery('.details .details-right').hide();
						}
					}
				}
				else {
					jQuery('.details .details-right').hide();
				}
			}
		});

            // Add placeholder values to the input fields and remove them on focus
            
                jQuery('#searchFirstName').val('Prénom');
                jQuery('#searchLastName').val('Nom de famille');
                jQuery('#teamSearchTeam').val('Nom d\'équipe');
            
            jQuery('#searchFirstName,#searchLastName,#teamSearchTeam').focus(function(){
                jQuery(this).val('');
                if (jQuery(this).attr('id') === 'searchFirstName' || jQuery(this).attr('id') === 'searchLastName') {
                    
                        jQuery('#teamSearchTeam').val('Nom d\'équipe');
                    
                }
                else if (jQuery(this).attr('id') === 'teamSearchTeam') {
                    
                        jQuery('#searchFirstName').val('Prénom');
                        jQuery('#searchLastName').val('Nom de famille');
                    
                }
            });
    
            jQuery('#findFriendForm').submit(function() {
    //			if(event.preventDefault) {
    //				event.preventDefault
    //			}
    //			else{
    //				event.returnValue=false;
    //			}
                var firstName = jQuery('#searchFirstName').val();
                var lastName= jQuery('#searchLastName').val();
                
                if (lastName != 'Last name' && lastName != 'Nom de famille') {
                    if (firstName === 'First name' || firstName === 'Prénom') {
                        jQuery('#searchFirstName').val('');
                    }
                }
                else if (firstName != 'First name' && firstName != 'Prénom') {
                    if (lastName = 'Last name' || lastName === 'Nom de famille') {
                        jQuery('#searchLastName').val('');
                    }
                }
                return true;
    
            });
    
    
        // Format the top 5 lists
    //	var lists = jQuery('#top-5-lists div.donor-list-indicator-container');
    
        jQuery('#top-5-lists').css('margin-top','30px');
    
        jQuery('.parts').hide();
		
		//trim team and part names 
		var trimNames = function(){
			jQuery('.teams a, .parts a').each(function(){
				var name = jQuery(this).text();
				if (name.length >=20){
					var trimmedName = name.substring(0,20); 
					jQuery(this).text(trimmedName + '...');
				}
			});
		}
		trimNames();
    
        function swap(){
            id = this.id;
            if (id === 'top-teams-title') {
                jQuery('#top-fundraisers-title, #top-teams-title').removeClass('active-list');
                jQuery(this).addClass('active-list');
                jQuery('#top-5-lists .teams, #see-all-teams').show();
                jQuery("#top-5-lists .parts").hide();
            }
            if (id === 'top-fundraisers-title') {
                jQuery('#top-fundraisers-title, #top-teams-title').removeClass('active-list');
                jQuery(this).addClass('active-list');
                jQuery('#top-5-lists .teams, #see-all-teams').hide();
                jQuery("#top-5-lists .parts").show();
        
            }
        }
    
        jQuery('#top-teams-title, #top-fundraisers-title').click(swap);

		
	
    });
    
    <!-- reusable notes: RFL_CAN_NATL_responsive_reus_teamraiser_js_css  RFL_CAN_NATL_responsive_reus_css  RFL_NATL_2015_reus_wrpr_head   RFL_CAN_NATL_reus_widget_top_five -->
    
    </script>
    
    
    
        
    



</div>
