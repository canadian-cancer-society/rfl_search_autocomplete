<script type="text/javascript">
Y.use("jquery-ui", function(Y) {
jQuery(function() {
var calcThermPercentage = function() {
// calculate the thermometer percentage
var raised = parseCurrency(jQuery('#2d7f2e63_thermometer .amount-raised-value').text());
var goal = parseCurrency(jQuery('#2d7f2e63_thermometer .total-goal-value').text());
var percent = raised / goal;
if (percent > 1) {
percent = 1;
}
var percentage = percent;
if(isNaN(percentage)) {
percentage = 0;
}
jQuery('#2d7f2e63_thermometer .amount-percentage-value').text(Math.floor(percentage * 100) + "%");
// get the width of the outer container;
var outerWidth = jQuery('#2d7f2e63_thermometer .thermometer-outer').css('width').slice(0, -2);
var innerWidth = outerWidth * percent;
jQuery('#2d7f2e63_thermometer .thermometer-inner').animate({'width': innerWidth + 'px'}, 1000);
}; 	
calcThermPercentage();
var waitForFinalEvent = (function () {
var timers = {};
return function (callback, ms, uniqueId) {
if (!uniqueId) {
uniqueId = "default_unique_id";
}
if (timers[uniqueId]) {
clearTimeout (timers[uniqueId]);
}
timers[uniqueId] = setTimeout(callback, ms);
};
})();
jQuery(window).resize(function () {
waitForFinalEvent(calcThermPercentage, 500, "2d7f2e63");
});
});
});
function parseCurrency(currency) {
currency = currency.replace(/[^0-9\.]+/g,"");
var val = parseInt(currency);
return val;
}
</script>
