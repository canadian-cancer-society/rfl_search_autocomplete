<!--    <script type="text/javascript">
    
        jQuery(document).ready(function($){
    
    //jQuery('#search_part_team').remove();
    
            function intify(str){
                var buf="";
                for(var i=0;i<str.length;i++){
                    if(str.charAt(i)!="$"&&str.charAt(i)!=","&&str.charAt(i).charCodeAt(0)!=160){
                        (str.charAt(i)==".")?i=str.length:buf+=str.charAt(i);
                    }
                }
                return buf;
            }
    
            function numberWithCommas(n) {
                var parts=n.toString().split(".");
                return parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",") + (parts[1] ? "." + parts[1] : "");
            }
    
    
            var locale = "fr_CA";
    
            // If sponsor and/or registration dollar amounts are included in the Sponsor 4 and 5 fields
            // of the greeting page, update the greeting page thermometer to include those amounts
    
            // Set a variable for the donation amount raised by the TeamRaiser 
            var dollars = '159 071,88 $';
//                console.log('dollars ' + dollars);
            var dollarsDecimal = '';
            // Manipulate the dollar amount into a decimal number appropriately based on language
            if (locale==='fr_CA'){
                var dollarsInt = parseInt(intify(dollars));
                dollarsDecimal = dollarsInt / 100;
            }
            else {
                var dollarsPlain = dollars.replace('$','').replace(',','');
//                    console.log('dollarsPlain ' + dollarsPlain);
                var dollarsFloat = parseFloat(dollarsPlain);
//                    console.log('dollarsFloat ' + dollarsFloat);
                dollarsDecimal = dollarsFloat;
                //dollarsDecimal = dollarsPlain;
            }
//                console.log('dollarsDecimal ' + dollarsDecimal);
    
            // Set a regular expression to test for numbers
            var numberRegex = /^[+-]?\d+(\.\d+)?([eE][+-]?\d+)?$/;
            // Set variables for any values in the Sponsor 4 and 5 fields
            var sponsor_4 = '';
            var sponsor_5 = '';
    
            // Create variables to hold sponsor amount and reg amount
            var sponsorAmount = '';
            var regAmount = '';
    
            // If the value in the sponsor 4 field is a number, set it as the sponsor amount
            if(numberRegex.test(sponsor_4)) {
                sponsorAmount = sponsor_4;
            }
            else {
                sponsorAmount = 0;
            }
//                console.log('sponsorAmount ' + sponsorAmount);
    
            // If the value in the sponsor 5 field is a number, set it as the registration amount
            if(numberRegex.test(sponsor_5)) {
                regAmount = sponsor_5;
            }
            else {
                regAmount = 0;
            }
//                console.log('regAmount ' + regAmount);
    
            // Add the three amounts to get the total dollar amount raised including
            // donations, sponsor dollars, and registrations and set decimal points to 2
            var totalAmount = (parseFloat(dollarsDecimal) + parseFloat(sponsorAmount) + parseFloat(regAmount)).toFixed(2);
//                console.log('totalAmount ' + totalAmount);
            
            // Change the total amount to a string so we can format it 
            var totalStr = String(totalAmount);
//                console.log('totalStr ' + totalStr);
    
            // Add commas for numbers greater than 999
            var commafy = numberWithCommas(totalStr);
//                console.log('commafy ' + commafy);
    
            if (locale === 'fr_CA') {
                // For French punctuation, replace the commas with spaces
                var addSpace = commafy.replace(/,/g,' ');
                // For French punctuation, replace the decimal with a comma
                var changeDecimal = addSpace.replace('.',',');
    
                // Put the total amount raised in the thermometer raised amount span
                jQuery('span.amount-raised-value').text(changeDecimal+' $');
                
                // Add a label for the goal
                jQuery('.total-goal-value').before('<span class="my-goal-label">Notre objectif : </span>');
            }
            else {
                // Put the total amount raised in the thermometer raised amount span
                jQuery('span.amount-raised-value').text('$'+commafy);
    
                // Add a label for the goal
                jQuery('.total-goal-value').before('<span class="my-goal-label">Our goal:</span>');
            }
   
            // Add a percent container to the thermometer
            jQuery('.thermometer-image').after('<div class="percent-container"></div>');     
    
            // Get the TeamRaiser goal
            var goal = "400 000,00 $";
            // Remove the dollar sign
            var goalPlain = goal.replace('$','');
    
            var updateGoal = '';
    
            if (locale === 'fr_CA') {
                // Swap the comma for a decimal point
                var goalDec = goalPlain.replace(',','.');
                // Make the goal a number
                var updateGoal = parseCurrency(goalDec);
            }
            else {
                var updateGoal = parseCurrency(goalPlain);
            }
    
            // Calculate the themometer percentage
            var percent=totalAmount/updateGoal;
//                console.log('percent ' + percent);
            if (percent > 1) {
                jQuery(".percent-container").html("100%");
            }
            else {
                jQuery(".percent-container").html(Math.round(percent*100)+"%");
            }
    
            // Get the sidebar thermometer and move it into place
            var sidebarThermo = jQuery('.side-bar #frStatus1').html();
            jQuery('#eventDetails #eventThermometer').append(sidebarThermo);
    
        });
   
    </script> -->

	


