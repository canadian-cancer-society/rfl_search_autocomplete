                          }
                          
                          var link2 = '';
    
    //				  var tracking = '&Source=RelayHP&Medium=Locator&Name=RegistrationTracking&Term=UserReg&Content=RegTrackandCheck';
                          var tracking = '&utm_source=RelayHP&utm_medium=Locator&utm_campaign=RegistrationTracking&utm_term=User Reg&utm_content=RegTrackandCheck';
    
    //                  var trGreeting = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry';
//                          var trGreeting = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry'+tracking;
                          var trGreeting = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry';
    
                          if (item.Year === '2014'){
//                              var trReg = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry'+tracking;
                              var trReg = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry';
                          }
                          else {
    //	                          var trReg = 'TR?type=fr_informational&pg=informational&fr_id='+item.TeamRaiserID+'&sid=7989'+tracking;
//                              var trReg = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry&reglink=true'+tracking;
                              var trReg = 'TR?fr_id='+item.TeamRaiserID+'&pg=entry&reglink=true';
                          }
    
                          // Handle links for SK events
                          if (item.Province === "SK") {
    
                             if (item.TeamRaiserID === 18123) {
                             // Handle the special case of Flin Flon, in SK but returned with MB events
                             // Get 2014 TeamRaiser ID for Flin Flon
                                   if (pType === ''){
                                       if (item.Comment.indexOf('reg closed, vol closed, lum closed') != -1){
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", trGreeting)
                                          .text(linkText);
                                       }
                                       else { 
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", "javascript:linkClicked(".concat(args.index, ")"))
                                          .text(linkText);
                                       }
                                   }
                                   if (pType.indexOf('reg') != -1){
                                      if (item.Comment.indexOf('reg closed') != -1){
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", trGreeting)
                                          .text(linkText);
                                      }
                                      else { 
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", "javascript:linkClicked(".concat(args.index, ")"))
                                          .text(linkText);
                                      }
                                   }
                                   if (pType.indexOf('pledgeEvent') != -1){
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", "javascript:linkClicked(".concat(args.index, ")"))
                                          .text(linkText);
                                   }
                                   if (pType.indexOf('luminary') != -1){
                                      if (item.Comment.indexOf('lum closed') != -1){
                                         link = jQuery("<a class='location-link'/>")
                                          .attr("href", trGreeting)
                                          .text(linkText);
                                      }
