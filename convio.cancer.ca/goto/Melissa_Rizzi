      <!-- ccsLocator begins -->
      <script type="text/javascript" src="//maps.googleapis.com/maps/api/js?key=AIzaSyCDDObSXiEtiefRol1dqKZWFum3cnuR0fs&sensor=false"></script>
             
          <script type="text/javascript" src="https://www.cancer.ca/resources/services/scripts/jquery.ccslocator-1.3.1.min.js"></script>
    
      <!-- ccsLocator begins -->
    
      <script type="text/javascript" src="../ccs/relayforlife_natl/js/jquery.parsequery.js"></script>
    
      <script type="text/javascript">
    
          // Global declarations follow.
    
          // jQuerylocator references the ccslocator created below.  It is used to retrieve data currently
          // rendered in the control.
          var jQuerylocator;
    
          // The linkClicked function receives the index of a location item associated with a link
          // in the locator sidebar or info window.  Data from the plugin is retrieved using the
          // ccsLocator function calls. Code can be added here to forward the visitor to the
          // appropriate page.
          function linkClicked(index) {
              // Get an array with the rendered locations from the locator plugin.
              var locations = jQuerylocator.ccsLocator("locations");
    
              // Get the location with the specified index.
              var item = locations[index];
    
              // Get the properties of the location as follows.
              var relayName = item.Name,            // The name of the relay (e.g. "Toronto Central").
                  year = item.Year,                 // The year of the relay (e.g. 2013).
                  address = item.Address,           // The location of the relay (e.g. "Sunnybrook Park").
                  city = item.City,                 // The city (e.g. "Toronto").
                  province = item.Province,         // The province code (i.e., "AB", "BC", "MB", "NB", "NL", "NS", "NU", "ON", "PE", "QC", "SK", or "YT").
                  eventDate = item.EventDate,       // The date of the relay formatted as a string (e.g. English: June 14, 2013 or French: 14 Juin, 2013).
                  eventTime = item.EventTime,       // The time of the relay (e.g. 7pm to 7am).  Users enter this in a text field, so the format may vary.
                  teamRaiserID = item.TeamRaiserID, // The team raiser id of the relay.
                  eventType = item.EventType,       // The event type. (i.e. "Community", "High School", or "Post-Secondary")
                  comment = item.Comment;           // The event comment.
    
              // We just render some of the properties here, but this is where you would add code to
              // compute the next URL and forward the user to that page.
              //alert("Code to navigate to next page goes here.\nRelay Name='".concat(relayName, "', Province='", province, "', TeamRaiserID='", teamRaiserID, "'."));
    
             var language = "fr_CA";
                 if (language === "") {
                      language = "en_CA";
                     }
             var formID = "";
             var storeID = "";
             var productID = ""; 
    
             // TO DO: Get links for SK 2014 events
    //           var linkSK = "http://www.ccsevents.ca/site/PageServer?pagename=RFL_13_eventlist";
             var linkSK = "https://secure3.convio.net/ccssk/site/SPageServer?pagename=RFL_13_homepage";
    //           var linkBC = "http://www.relaybc.ca/site/c.asILI3NKKgK2F/b.6725663/k.BE2B/Home.htm";
    
             // 2015 Blueprint is 16876
             if (teamRaiserID === 16876) {
                formID = " 24243";
                storeID = "23741";
                productID = "4241";
                }
             else if (province === "AB" || province === "NT") {
                if (year === 2014) {
                     formID = "21841";
                     storeID = "22321";
                     productID = "3721";
                }
                else {
                     formID = "24201";
                     storeID = "23842";
                     productID = "4341";
                }
             }
             else if (province === "BC" || province === "YT") {
                if (year === 2014) {
                     formID = "21922";
                     storeID = "22362";
                     productID = "3741";
                }
                else {
                     formID = "24180";
                     storeID = "23822";
                     productID = "4301";
                }
             }
             else if (province === "MB") {
                if (year === 2014) {
                     formID = "21580";
                     storeID = "22201";
                     productID = "3621";
                }
                else {
                     formID = "24561";
                     storeID = "24081";
                     productID = "4421";
                }
             }
             else if (province === "NB") {
                if (year === 2014) {
                     formID = "21600";
                     storeID = "22221";
                     productID = "3641";
                }
                else {
                     formID = "24300";
                     storeID = "23902";
                     productID = "4361";
                }
             }
             else if (province === "NL") {
                formID = "26440";
                storeID = "25321";
                productID = "4761";
                }
             else if (province === "NS") {
                if (year === 2014) {
                     formID = "21622";
                     storeID = "22241";
                     productID = "3661";
                }
                else {
                     formID = "24581";
                     storeID = "24101";
                     productID = "4441";
                }
             }
             else if (province === "ON" || province === "NU") {
                if (year === 2016) {
                     formID = "27100";
                     storeID = "25701";
                     productID = "4841";
                }
                else {
                     formID = "24382";
                     storeID = "23922";
                     productID = "4381";
                }
             }
             else if (province === "PE") {
                if (year === 2014) {
                    formID = "22860";
                    storeID = "22881";
                    productID = "3501";
                }
                else {
                    formID = "25381";
                    storeID = "24561";
                    productID = "4501";
                }
             }
             else if (province === "QC") {
                if (year === 2014) {
                    formID = "22202";
                    storeID = "22441";
                    productID = "3801";
                }
                else {
                     formID = "24822";
                     storeID = "24221";
                     productID = "4461";
                }
            }
             else if (province === "SK") {
                 if (teamRaiserID === 12297) {
                   formID = "20065";
                   storeID = "21401";
                   productID = "3381";
                }
                else {
                   formID = "SKffff";
                   storeID = "SKssss";
                   productID = "SKpppp";
                }
             }
             else {
                formID = "ffff";
                storeID = "ssss";
                productID = "pppp";
             }
    
              //Set the participation type variable
              
               var pType="";
               
    
             //Set the options for each pType
             switch (pType)
             {
             case "":
               if (province === "SK") {
                  if (teamRaiserID === 18123){
                  // 2015 TeamRaiser ID for Flin Flon. Flin Flon does not have a 2014 event.
                      window.location = "TR?fr_id=" + teamRaiserID + "&pg=entry&s_locale=" + language;
                  }
                  else {window.location = linkSK;
                  } 
               }
    //             else if (province === "BC" || province === "YT") {
    //				 if (year === 2013) {
    //                	window.location = linkBC; 
    //				 }
    //				 else {
    //                	window.location = "TR?fr_id=" + teamRaiserID + "&pg=entry&s_locale=" + language;					 
    //				 }
    //             }
               else {
                  window.location = "TR?fr_id=" + teamRaiserID + "&pg=entry&s_locale=" + language;
               }
               break;
           
             case "regJoinTeam":
               if (province === "SK") {
                  // 2015 TeamRaiser ID for Flin Flon. Flin Flon does not have a 2014 event.
                  if (teamRaiserID === 18123){
                      window.location = "TRR?fr_id=" + teamRaiserID + "&s_corporate=no&pg=tfind&s_reclaim=no&s_survivor=no&s_promoCode=Participant&s_locale=" + language;
                  }
                  else {
                      window.location = linkSK;
                  }
               }
