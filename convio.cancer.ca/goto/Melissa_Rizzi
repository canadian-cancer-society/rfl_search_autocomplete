                   var viewList = '<div class="full-list-link clearme"><a class="left" href="PageServer?pagename=RFL_NATL_2015_event_list&s_locale=fr_CA">Consultez la liste des événements communautaires</a><a class="right" href="PageServer?pagename=RFLY_NATL_2015_event_list&s_locale=fr_CA">Consultez la liste des événements dans les écoles</a></div>';
               
                jQuery('#ccs-locator-container').append(viewList);
    
