    jQuery(document).ready(function() {
    
        var name = '';
        
        
            // outside PC2 personal page preview, use these variables
            var firstName = 'Judy';
        
    
        var screenname =  '';
        
        if (screenname) {
            name = screenname;
        }
        else {
            name = firstName;
        }
        
        var splitName = name.split('');
        var nameCheck = splitName[splitName.length-1];
    
        
            var anonymous = 'false';
        
        
        
             
                
                    jQuery('#personal_page_header h2').html("Aidez  " + name + " &agrave; prendre le Relais");
                
            
        
        
        //are there any custom badges? 
        var customBadgeCheck = jQuery('#movebadges .custom-badge').length;
        console.log('customBadgeCheck ' + customBadgeCheck);
        //are badges enabled for this TR in general?
        var badgeCheck = '0';
        console.log('badgeCheck ' + badgeCheck);
        
        var earnedBadges = jQuery('.TrAchievementBadgeLarge').length;
        console.log('earnedBadges for 18613 ' + earnedBadges);
    
        //function to move custom badges into achievements area
        var moveBadges = function() {
            console.log('running moveBadges');
            jQuery('#movebadges .custom-badge').each(function() {
                var badge = jQuery(this).html();
//                jQuery('.TrAchievementBadgeLarge').append('<div class="custom-badge clearfix">'+badge+'</div>');
				jQuery('.TrAchievementBadgeLarge').last().append('<div class="custom-badge clearfix">'+badge+'</div>');
            });
        }
    
    //    if (badgeCheck > 0){
            // If badges are enabled for this TeamRaiser
            if (jQuery('.TrAchievementBadgeLarge').length === 0 && customBadgeCheck === 0){
                // if no milestone badges and no custom badges exist, just rewrite the badge container HTML
                console.log('found no badges');
                jQuery('.TrAchievementBadgeIconListLarge').html('<div class="TrAchievementBadgeIconListLargeHeader">Accomplissements</div><div class="TrAchievementBadgeLarge">Aucun \u00E9cusson n\'a \u00E9t\u00E9 aquis</div>');
            }
            else if (jQuery('.TrAchievementBadgeLarge').length === 0 && customBadgeCheck > 0){
                // if no milestone badges are earned but at least one custom badge exists
                // rewrite the badge container HTML and add move in the custom badge
                console.log('found only customs');
                jQuery('.TrAchievementBadgeIconListLarge').html('<div class="TrAchievementBadgeIconListLargeHeader">Accomplissements</div><div class="TrAchievementBadgeLarge clearfix"></div>');
                moveBadges();
            }
            else if (jQuery('.TrAchievementBadgeLarge').length > 0 && customBadgeCheck > 0){
                // if at least one milestone badge exists and at least one custom badge exists
                // rewrite the badge container HTML and add move in the custom badge
                console.log('found some milestones and customs');
                jQuery('.TrAchievementBadgeLarge').addClass('clearfix');
                moveBadges();
            }
     //   }
        
        jQuery('div.non-mobile-donate-button-container').wrap('<div class="corners"><div class="don-gradient"></div></div>');
        jQuery('#personal_join_team_button_container').wrap('<div class="corners"><div class="reg-gradient"></div></div>');
    
        var eventDetails = jQuery('#movedetails').html();
        if (jQuery('#personal_page_image_div').length > 0) {
            jQuery('#personal_page_image_div').after('<div id="event-details">'+eventDetails+'</div>');
            jQuery('.manageable-editor-window-editor').addClass('clearme');
        }
        else {
            jQuery('.manageable-editor-window-editor').before('<div id="event-details">'+eventDetails+'</div>');
            jQuery('#event-details').css({'float':'none','width':'100%','margin-left':'0','margin-bottom':'30px'});
        }
    
        jQuery('#personal_page_team_page_link').html('Visitez la page d\'&eacute;quipe');
    
        var top5 = jQuery('#movetop5').html();
        jQuery('#personal_page_main_content').append('<div id="newtop5">'+top5+'</div>');
    
        var goal = '10 000,00 $';
        var percentGoal = '0';
        var amount = '0,00 $';
    
        
//            var goalLabel = 'l\'objectif'
            var goalLabel = 'Objectif'
            var percentLabel = ' de l\'objectif';
            var raisedLabel = ' Recueilli';
        
    //	var thermInfo = jQuery('.thermometer-container .text').html();
    //	console.log('thermInfo ' + thermInfo);
    
        jQuery('.total-goal-info').append('<span class=total-goal-value-label>'+goalLabel+'</span>');
    
        jQuery('.donation-link-container').after('<div class="amt-info clearfix">'
        + '<div class="percentage-info left">'
