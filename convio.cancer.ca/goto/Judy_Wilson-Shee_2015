                                  break;
                  case 'QC':
                                  //qc code here
                                  sUACode = 'UA-66852536-1';
                                  break;
                  default:
                                  //unknown state/province
                                  sUACode = '';
  }
  if (sUACode.length > 0) {
                ga('create', sUACode, {name: 'newTracker'});
                ga('newTracker.require', 'displayfeatures');
                ga('newTracker.require', 'linkid', 'linkid.js', 'linker');
                ga('linker:autoLink', ['convio.cancer.ca', 'secure2.convio.net'] );
                ga('newTracker.send', 'pageview',{
                                'location':sLocation,
                                'page':sPage,
                                'title':sTitle
                });
  }
	console.log('sUACode ' + sUACode);
</script>















 
		
</head>
